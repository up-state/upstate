{
  "name": "vite-nest",
  "version": "0.0.0",
  "scripts": {
    "dev:server": "npx ts-node-dev --project ./tsconfig.server.json ./src/server/main.ts",
    "dev:client": "vite",
    "dev": "concurrently npm:dev:server npm:dev:client",
    "build:client": "vue-tsc --noEmit && vite build",
    "build:server": "tsc --project ./tsconfig.server.build.json",
    "prebuild": "rimraf ./dist/*",
    "build": "concurrently npm:build:server npm:build:client",
    "start": "NODE_ENV=production node ./dist/server/main.js",
    "serve": "vite preview",
    "test:client": "jest src/client"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.11",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.6",
    "concurrently": "^6.0.2",
    "identity-obj-proxy": "^3.0.0",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vue": "^3.0.11",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-tsc": "^0.0.24"
  }
}
